   "source": [
    "def beat_transformer(df):\n",
    "    df['BEAT_OF_OCCURENCE'] = df['BEAT_OF_OCCURENCE'].astype(str)\n",
    "    df['BEAT_OF_OCCURENCE'] = df['BEAT_OF_OCCURENCE'].apply(lambda x: x[:-4])\n",
    "    return df"
   ]
  },
  {
